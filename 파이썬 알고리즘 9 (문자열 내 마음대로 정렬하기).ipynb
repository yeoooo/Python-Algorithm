    "def solution(s, n):\n",
    "    answer = []\n",
    "    \n",
    "    for i in range(len(s)):\n",
    "        s[i] = s[i][n] + s[i]\n",
    "    \n",
    "    s.sort()\n",
    "    \n",
    "    for i in range(len(s)):\n",
    "        answer.append(s[i][1:])        \n",
    "            \n",
    "    return answer"
   ]
